{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
      {
        "name": "prod",
        "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/staywise-api-prod",
        "hostname": "prod",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ]
      }
    ]
  }